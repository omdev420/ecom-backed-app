{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "implicit-arrow-linebreak": "off",
    "no-console": "warn",
    "comma-dangle": "off",
    "function-paren-newline": "off"
  }
}
